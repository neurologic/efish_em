def grayscale_cmap_hex(num_colors=256):
    """
    Generates a grayscale colormap and returns a list of hex codes.

    Args:
        num_colors: The number of colors in the colormap.

    Returns:
        A list of hex codes representing the grayscale colormap.
    """
    cmap = plt.cm.get_cmap('gray', num_colors)
    hex_colors = [matplotlib.colors.to_hex(cmap(i)) for i in range(cmap.N)]
    return hex_colors





